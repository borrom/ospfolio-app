<p id="notice"><%= notice %></p>

<h1>Listing Intros</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Subtitle</th>
      <th>Desctitle</th>
      <th>Btnmain</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intros.each do |intro| %>
      <tr>
        <td><%= intro.title %></td>
        <td><%= intro.subtitle %></td>
        <td><%= intro.desctitle %></td>
        <td><%= intro.btnmain %></td>
        <td><%= link_to 'Show', intro %></td>
        <td><%= link_to 'Edit', edit_intro_path(intro) %></td>
        <td><%= link_to 'Destroy', intro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intro', new_intro_path %>
